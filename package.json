{
  "name": "oli-gdocs-converter",
  "version": "1.0.0",
  "description": "OLI Google Docs converter tool. Transforms formated Docs to corresponding OLI workbook pages",
  "main": "es6-init.js",
  "scripts": {
    "start": "rm -rf .cache && cross-env NODE_ENV=development electron .",
    "clean-cache": "rm -rf .cache",
    "clean-source": "node cleaner.js \"./dist\"",
    "compile": "cross-env NODE_ENV=production electron-compile --appDir . ./src",
    "package": "run-s clean-cache compile package-osx package-linux package-win clean-source",
    "package-linux": "cross-env NODE_ENV=production electron-packager . oli-gdocs-converter --version 1.4.10  --out=dist --platform=linux --arch=all --icon ./src/assets/images/logo.png --overwrite --ignore=\".compilerc|.gitignore|cleaner.js|.DS_Store|.idea|nbproject\"",
    "package-win": "cross-env NODE_ENV=production electron-packager . oli-gdocs-converter --version 1.4.10  --out=dist --platform=win32 --arch=all --icon ./src/assets/images/logo.ico --overwrite --ignore=\".compilerc|.gitignore|cleaner.js|.DS_Store|.idea|nbproject\"",
    "package-osx": "cross-env NODE_ENV=production electron-packager . oli-gdocs-converter --version 1.4.10  --out=dist --platform=darwin --arch=all --icon ./src/assets/images/logo.icns --overwrite --ignore=\".compilerc|.gitignore|cleaner.js|.DS_Store|.idea|nbproject\"",
    "quick-package": "run-s clean-cache compile package-osx clean-source",
    "package-for-this": "cross-env NODE_ENV=production electron-packager . oli-gdocs-converter --version 1.4.10 --out=dist --overwrite --ignore=\".compilerc|.gitignore|cleaner.js|.DS_Store|.idea|nbproject\""
  },
  "devDependencies": {
    "archiver": "^1.2.0",
    "asar": "^0.12.3",
    "babel": "^6.5.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-preset-react": "^6.16.0",
    "cross-env": "^3.1.3",
    "electron": "^1.4.10",
    "electron-compilers": "^4.0.5",
    "electron-packager": "^8.3.0",
    "npm-run-all": "^3.1.2"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "electron-compile": "^4.0.2",
    "fs-extra": "^1.0.0",
    "jsdom": "^9.8.3",
    "pify": "^2.3.0",
    "pretty-data": "^0.40.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "unzip": "^0.1.11"
  }
}
